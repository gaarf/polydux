{
  "name": "polystuff",
  "description": "PolyStuff",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "repository": "gaarf/polystuff",
  "devDependencies": {
    "eslint": "^3.12.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-html": "^1.7.0",
    "web-component-tester": "6.0.0-prerelease.7"
  },
  "scripts": {
    "lint": "eslint . --ext js,html --ignore-path .gitignore",
    "test": "npm run lint && polymer test",

    "repl": "node ops/repl.js --debug=0",
    "create-user": "node ops/createUser.js --debug=0",

    "prebuild": "npm install && polymer install",
    "build": "polymer build",
    "prestart": "node server/express.js --expressPort 8001 &",
    "start": "polymer serve -p 8088 --proxy-target http://localhost:8001/api --proxy-path /api  -o --open-path /"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cached": "^4.3.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "consolidate": "^0.14.5",
    "envar": "^2.0.0",
    "express": "^4.15.2",
    "express-restify-mongoose": "^4.1.3",
    "express-session": "^1.15.2",
    "finalhandler": "^1.0.1",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.4",
    "morgan": "^1.8.1",
    "needle": "^1.6.0",
    "passport": "^0.3.2",
    "passport-local-mongoose": "^4.0.0",
    "q": "^1.5.0",
    "redux": "^3.6.0"
  }
}
